    "babel-eslint": "~8.0.3",
    "babel-loader": "~7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-remove-strict-mode": "~0.0.2",
    "babel-preset-env": "~1.6.1",
    "body-parser": "~1.18.2",
    "chai": "~4.1.2",
    "clipboard": "^1.5.15",
    "codemirror": "*",
    "commander": "~2.12.2",
    "connect": "~3.6.6",
    "d3-queue": "~3.0.7",
    "debug": "~3.1.0",
    "electron": "~2.0.2",
