    "jszip": "~3.1.5",
    "nightmare": "~2.10.0",
    "nunjucks": "~3.0.1",
    "open": "~0.0.5",
    "parseurl": "~1.3.2",
    "pre-commit": "~1.2.2",
    "radixsort": "~1.0.0",
    "serve-static": "~1.13.2",
    "shelljs": "~0.7.8",
    "string-replace-loader": "~1.3.0",
