{
  "name": "@antv/g6",
  "version": "5.0.0-alpha.9",
  "description": "A Graph Visualization Framework in JavaScript",
  "keywords": [
    "antv",
    "g6",
    "graph",
    "graph analysis",
    "graph editor",
    "graph visualization",
    "relational data"
  ],
  "homepage": "https://g6.antv.antgroup.com/",
  "bugs": {
    "url": "https://github.com/antvis/g6/issues"
  },
  "repository": "git@github.com/antvis/g6.git",
  "license": "MIT",
  "author": "https://github.com/orgs/antvis/people",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "src",
    "esm",
    "lib",
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "start": "father build --watch",
    "build:umd": "rimraf ./dist && rollup -c && npm run size",
    "build:cjs": "rimraf ./lib && tsc --module commonjs --outDir lib",
    "build:esm": "rimraf ./esm && tsc --module ESNext --outDir esm",
    "build": "run-p build:*",
    "bundle-vis": "cross-env BUNDLE_VIS=1 run-p build:umd",
    "prepublishOnly": "npm run ci",
    "ci": "run-s lint build",
    "clean": "rimraf es lib",
    "coverage": "jest --coverage",
    "doc": "rimraf apis && typedoc",
    "lint": "eslint ./src --quiet && prettier ./src --check",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "fix": "eslint ./src ./tests --fix && prettier ./src ./tests --write ",
    "test": "jest",
    "test:integration": "node --expose-gc --max-old-space-size=4096 --unhandled-rejections=strict node_modules/jest/bin/jest tests/integration/ --config jest.node.config.js --coverage -i  --logHeapUsage",
    "size": "limit-size",
    "test-live": "DEBUG_MODE=1 jest --watch  ./tests/unit/edge-spec.ts",
    "test-behavior": "DEBUG_MODE=1 jest --watch ./tests/unit/item-3d-spec.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/colors": "^7.0.0",
    "@antv/algorithm": "^0.1.25",
    "@antv/dom-util": "^2.0.4",
    "@antv/g": "^5.15.7",
    "@antv/g-canvas": "^1.9.28",
    "@antv/g-plugin-3d": "^1.7.45",
    "@antv/g-plugin-control": "^1.7.43",
    "@antv/g-plugin-dragndrop": "^1.8.1",
    "@antv/g-svg": "^1.8.36",
    "@antv/g-webgl": "^1.7.44",
    "@antv/graphlib": "^2.0.2",
    "@antv/gui": "^0.5.0-alpha.16",
    "@antv/layout": "^1.2.0",
    "@antv/layout-gpu": "^1.0.0",
    "@antv/layout-wasm": "1.3.1",
    "@antv/matrix-util": "^3.0.4",
    "@antv/util": "~2.0.5",
    "color": "^4.2.3",
    "tslib": "^2.5.0",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@antv/g6": "^4.8.13",
    "@kayahr/jest-electron-runner": "^5.1.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "13.11.1",
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@umijs/fabric": "^2.0.0",
    "babel-loader": "^8.0.6",
    "canvas": "2.11.0",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "father": "^2.29.1",
    "jest": "^28.1.3",
    "jest-extended": "^0.11.2",
    "limit-size": "^0.1.4",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "pixelmatch": "5.3.0",
    "pngjs": "^6.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.0",
    "rollup": "^2.79.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-polyfill-node": "^0.8.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "stats.js": "^0.17.0",
    "ts-jest": "^28.0.8",
    "typedoc": "^0.24.0",
    "typescript": "^4.9.5",
    "vite": "^4.2.2",
    "xmlserializer": "^0.6.1"
  },
  "limit-size": [
    {
      "path": "dist/g6.min.js",
      "limit": "750 Kb",
      "gzip": true
    },
    {
      "path": "dist/g6.min.js",
      "limit": "2 Mb"
    }
  ]
}
