{
  "name": "@antv/g6",
  "version": "5.0.0-beta.28",
  "description": "A Graph Visualization Framework in JavaScript",
  "keywords": [
    "antv",
    "g6",
    "graph",
    "graph analysis",
    "graph editor",
    "graph visualization",
    "relational data"
  ],
  "homepage": "https://g6.antv.antgroup.com/",
  "bugs": {
    "url": "https://github.com/antvis/g6/issues"
  },
  "repository": "git@github.com/antvis/g6.git",
  "license": "MIT",
  "author": "https://github.com/orgs/antvis/people",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "src",
    "esm",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:cjs": "rimraf ./lib && tsc --module commonjs --outDir lib",
    "build:esm": "rimraf ./esm && tsc --module ESNext --outDir esm",
    "build:esm:watch": "rimraf ./esm && tsc --module ESNext --outDir esm --watch",
    "build:umd": "rimraf ./dist && rollup -c && npm run size",
    "bundle-vis": "cross-env BUNDLE_VIS=1 npm run build:umd",
    "ci": "run-s lint build test",
    "coverage": "jest --coverage",
    "dev": "vite",
    "fix": "eslint ./src ./__tests__ --fix && prettier ./src __tests__ --write ",
    "jest:base": "node --expose-gc --max-old-space-size=4096 --unhandled-rejections=strict --experimental-vm-modules ../../node_modules/jest/bin/jest --coverage --logHeapUsage --detectOpenHandles",
    "lint": "eslint ./src __tests__ --quiet && prettier ./src __tests__ --check",
    "prepublishOnly": "npm run ci",
    "size": "limit-size",
    "start": "rimraf ./lib && tsc --module commonjs --outDir lib --watch",
    "test": "run-s test:*",
    "test:integration": "npm run jest:base __tests__/integration",
    "test:unit": "npm run jest:base __tests__/unit"
  },
  "dependencies": {
    "@antv/algorithm": "^0.1.26",
    "@antv/event-emitter": "latest",
    "@antv/g": "^5.18.23",
    "@antv/g-canvas": "^1.11.25",
    "@antv/g-plugin-dragndrop": "^1.8.17",
    "@antv/graphlib": "^2.0.2",
    "@antv/hierarchy": "latest",
    "@antv/layout": "^1.2.12",
    "@antv/util": "^3.3.5"
  },
  "devDependencies": {
    "@antv/g-plugin-3d": "^1.9.26",
    "@antv/g-plugin-control": "^1.9.17",
    "@antv/g-svg": "^1.10.21",
    "@antv/g-webgl": "^1.9.29",
    "@types/xmlserializer": "^0.6.6",
    "stats.js": "^0.17.0",
    "vite": "^5.0.10",
    "xmlserializer": "^0.6.1"
  },
  "limit-size": [
    {
      "path": "dist/g6.min.js",
      "limit": "750 Kb",
      "gzip": true
    },
    {
      "path": "dist/g6.min.js",
      "limit": "2 Mb"
    }
  ]
}
