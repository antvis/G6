{
  "name": "@antv/g6",
  "version": "5.0.0-beta.1",
  "description": "A Graph Visualization Framework in JavaScript",
  "keywords": [
    "antv",
    "g6",
    "graph",
    "graph analysis",
    "graph editor",
    "graph visualization",
    "relational data"
  ],
  "homepage": "https://g6.antv.antgroup.com/",
  "bugs": {
    "url": "https://github.com/antvis/g6/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antvis/g6"
  },
  "license": "MIT",
  "author": "https://github.com/orgs/antvis/people",
  "files": [
    "src",
    "es",
    "lib",
    "dist"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "father build --watch",
    "build": "npm run clean && father build",
    "ci": "npm run build && npm run coverage",
    "clean": "rimraf es lib",
    "coverage": "jest --coverage",
    "doc": "rimraf apis && typedoc",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --format=pretty \"./\"",
    "lint:src": "eslint --ext .ts --format=pretty \"./src\"",
    "prettier": "prettier -c --write \"**/*\"",
    "test": "jest",
    "test-live": "DEBUG_MODE=1 jest --watch  ./tests/unit/theme-subject-spec.ts",
    "test-behavior": "DEBUG_MODE=1 jest --watch ./tests/unit/click-select-spec.ts",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "watch": "father build -w"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged & npm run test"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npm run lint-staged:js"
    ]
  },
  "dependencies": {
    "@ant-design/colors": "^7.0.0",
    "@antv/g": "^5.15.7",
    "@antv/g-canvas": "^1.9.28",
    "@antv/g-svg": "^1.8.36",
    "@antv/g-webgl": "^1.7.44",
    "@antv/graphlib": "^2.0.0",
    "@antv/layout": "^1.0.0-alpha.17",
    "@antv/layout-gpu": "^1.0.0-alpha.3",
    "@antv/util": "~2.0.5",
    "color": "^4.2.3",
    "typedoc-plugin-markdown": "^3.14.0"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "13.11.1",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@umijs/fabric": "^2.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "eslint": "^7.7.0",
    "event-simulate": "~1.0.0",
    "father": "^2.29.1",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-electron": "^0.1.7",
    "jest-extended": "^0.11.2",
    "lint-staged": "^10.2.11",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.0",
    "stats-js": "^1.0.1",
    "ts-jest": "^24.1.0",
    "ts-loader": "^7.0.3",
    "typedoc": "^0.23.24",
    "typescript": "^4.6.3"
  }
}
